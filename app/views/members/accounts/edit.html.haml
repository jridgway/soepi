= render :partial => "/members/accounts/common"

- content_for :body_content do
  = simple_form_for(resource, :html => {:method => :put, :multipart => true, :novalidate => true, :class => 'validate'}, :as => resource_name, :url => registration_path(resource_name)) do |f|
    = render :partial => '/members/accounts/form', :locals => {:f => f}
    .actions
      = f.submit 'Update', :class => 'button'
      
= render :partial => "/shared/content_page"
